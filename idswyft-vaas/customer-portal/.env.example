# VaaS Customer Portal Environment Variables

# API Configuration
VITE_API_URL=https://api-vaas.idswyft.app
VITE_API_TIMEOUT=30000

# Idswyft Verification API Configuration
# Main Idswyft API base URL (for real verification processing)
VITE_API_BASE_URL=https://api.idswyft.app
# Organization's Idswyft API key (fallback if not in organization settings)
VITE_IDSWYFT_API_KEY=your_idswyft_api_key_here
# Enable sandbox mode for testing (uses test verification engine)
VITE_SANDBOX_MODE=true

# Environment
VITE_NODE_ENV=production

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/jpg,image/png,application/pdf

# Camera Settings
VITE_ENABLE_LIVENESS_DETECTION=true
VITE_CAMERA_QUALITY=0.8
VITE_CAPTURE_FORMAT=image/jpeg

# UI Settings
VITE_BRAND_NAME=Idswyft Verification
VITE_SUPPORT_EMAIL=support@idswyft.app
VITE_SUPPORT_PHONE=

# Features
VITE_DEBUG_MODE=false
VITE_ENABLE_MOCK_CAMERA=false

# Analytics (optional)
VITE_ANALYTICS_ID=your_analytics_id

# Error Reporting (optional)
VITE_SENTRY_DSN=your_sentry_dsn